{
    "name": "tp/ident",
    "description": "A library to add identity to objects",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Ploch",
            "email": "profiploch@gmail.com"
        }
    ],
    "keywords": [
        "identification", "identity", "id"
    ],
    "require": {
        "php": ">=5.5",
        "nikic/iter": "~1.1",
        "rhumsaa/uuid": "~2.0",
        "jms/metadata": "~1.1",
        "doctrine/annotations": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.5",
        "symfony/security": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "pimple/pimple": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Ident\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ident\\Test\\": "test/"
        }
    },
    "suggest": {
        "doctrine/dbal": "Allow the use of a UUID as doctrine field type."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
