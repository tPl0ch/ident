{
    "name": "tPl0ch/ident",
    "description": "An abstraction layer for adding identity to objects",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Ploch",
            "email": "profiploch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "nikic/iter": "~1.1",
        "rhumsaa/uuid": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Ident\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ident\\Test\\": "test/"
        }
    },
    "suggest": {
        "doctrine/dbal": "Allow the use of a UUID as doctrine field type."
    }
}
